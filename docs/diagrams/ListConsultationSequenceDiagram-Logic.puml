@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "d:ListConsultationCommand" as ListConsultationCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("list_consult")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("list_consult")
activate AddressBookParser

create ListConsultationCommand
AddressBookParser -> ListConsultationCommand
activate ListConsultationCommand

ListConsultationCommand --> AddressBookParser :
deactivate ListConsultationCommand

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

LogicManager -> ListConsultationCommand : execute(m)
activate ListConsultationCommand

ListConsultationCommand -> Model : updateFilteredConsultationList
activate Model

Model --> ListConsultationCommand
deactivate Model

create CommandResult
ListConsultationCommand -> CommandResult
activate CommandResult

CommandResult --> ListConsultationCommand
deactivate CommandResult

ListConsultationCommand --> LogicManager : r
deactivate ListConsultationCommand

[<--LogicManager
deactivate LogicManager
@enduml
